<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC 
    "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" 
    "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd">

<hibernate-reverse-engineering>
    <table-filter match-name=".*"/>

    <table catalog="laravel" name="asignatura_usuario_horario">
        <column name="id"/>
        <column name="usuario_id"/>
        <column name="horario_id"/>
        <column name="asignatura_id"/>
    </table>
    
    <table catalog="laravel" name="asignaturas">
        <column name="id"/>
        <column name="nombre"/>
        <column name="descripcion"/>
        <column name="ciclo_id"/>
    </table>
    
    <table catalog="laravel" name="ciclo_usuario">
        <column name="id"/>
        <column name="fecha_matricula"/>
        <column name="ciclo_id"/>
        <column name="usuario_id"/>
    </table>
    
    <table catalog="laravel" name="ciclos">
        <column name="id"/>
        <column name="nombre"/>
        <column name="curso"/>
        <column name="descripcion"/>
    </table>
    
    <table catalog="laravel" name="horarios">
        <column name="id"/>
        <column name="dia_semana"/> <!-- Cambiado de dia-semana -->
        <column name="hora"/>
        <column name="profesor_id"/>
        <column name="asignatura_id"/>
    </table>
    
    <table catalog="laravel" name="reunions">
        <column name="id"/>
        <column name="comentario"/>
        <column name="estado"/>
        <column name="fecha_hora_inicio"/> <!-- Cambiado de fecha-hora-inicio -->
        <column name="fecha_hora_fin"/> <!-- Cambiado de fecha-hora-fin -->
        <column name="emisor_id"/>
        <column name="receptor_id"/>
    </table>
    
    <table catalog="laravel" name="rols">
        <column name="id"/>
        <column name="name"/>
    </table>
    
    <table catalog="laravel" name="users">
        <column name="id"/>
        <column name="user"/>
        <column name="surname"/>
        <column name="tel_1"/> <!-- Cambiado de tel-1 -->
        <column name="tel_2"/> <!-- Cambiado de tel-2 -->
        <column name="address"/>
        <column name="dni"/>
        <column name="name"/>
        <column name="email"/>
        <column name="password"/>
        <column name="photo"/>
        <column name="role_id"/>
    </table>
</hibernate-reverse-engineering>
